{
	"expo": {
		"scheme": "expo-scratch",
		"userInterfaceStyle": "automatic",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"splash": {
			"image": "./assets/images/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#FFFFFF"
			}
		},
		"web": {
			"output": "static",
			"bundler": "metro",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			[
				"expo-router",
				{
					"origin": "https://n"
				}
			]
		],
		"name": "ReactNativeExpoScratch",
		"slug": "ReactNativeExpoScratch",
		"experiments": {
			"typedRoutes": true,
			"tsconfigPaths": true
		}
	}
}
